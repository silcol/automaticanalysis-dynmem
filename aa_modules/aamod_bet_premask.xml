<?xml version="1.0" encoding="utf-8"?>
<aap>
    <tasklist>
        <currenttask domain='subject' desc='Masks the structural by the T1 template, removing the neck' modality='MRI'>
            
            <qsub>
                <timeBase>0.1</timeBase>
                <memoryBase>0.5</memoryBase>
            </qsub>
            
            <permanenceofoutput>1</permanenceofoutput>
            
            <inputstreams>
                <stream>structural</stream>
            </inputstreams>
            
            <outputstreams>
                <stream>structural</stream>
            </outputstreams>
            
            <!-- This specifies which structural we want to use, typically the first, but can be changed 
            Can add multiple, but then only first one will be used for BETting, but operation will be applied on both -->
            <structural>1</structural>
            
            <diagnostic>0</diagnostic>
            
        </currenttask>
    </tasklist>
