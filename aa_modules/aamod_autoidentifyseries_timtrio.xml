<?xml version="1.0" encoding="utf-8"?>
<aap>
    <tasklist>
        <currenttask domain='subject' mfile_alias='aamod_autoidentifyseries_timtrio' desc='Automatically identify series numbers'>
            
            <qsub>
                <timeBase>0.2</timeBase>
                <memoryBase>0.3</memoryBase>
            </qsub>
            
            <permanenceofoutput>-1</permanenceofoutput>
            
            <!-- This is typically SeriesDescription for GE, and ProtocolName for Siemens -->
            
            <dicom_protocol_field>ProtocolName</dicom_protocol_field>
            <orderdicomsby>AcquisitionNumber</orderdicomsby>
            
            <outputstreams>
                <stream>autoidentifyseries</stream>
            </outputstreams>
            
        </currenttask>
    </tasklist>
</aap>
